<div style="padding:2%;">
	<div class="row">
		<div class="col m4">
			<h2>Total Users: {{totalUsers}}</h2>
		</div>
		<div class="col m4">
			<h2>Total Stations: {{totatStations}}</h2>
		</div>
		<div class="col m4">
			<h2>Total Sensors Types: 9</h2>
		</div>
	</div>
	<div class="row">
		<div class="col m6">
			<h4>Top Users</h4>
			  <ul class="collapsible" data-collapsible="accordion">
				  <li ng-repeat="data in adminHistory.topUsers">
					  <div class="collapsible-header">{{data.firstname + data.lastname}}</div>
					  <div class="collapsible-body">
						  <p>Name: {{data.firstname}}</p>
						  <p>Email: {{data.email}}</p>
						  <p>No of Times Visited : {{data.counter}}</p>
					  </div>
				  </li>
			  </ul>
		</div>
		<div class="col m6">
			<h4>Top Stations</h4>
			<ul class="collapsible" data-collapsible="accordion">
				<li ng-repeat="data in adminHistory.topStations">
					<div class="collapsible-header">{{data.stationName}}</div>
					<div class="collapsible-body">
						<p>StationId: {{data.stationId}}</p>
						<p>Name: {{data.stationName}}</p>
						<p>Status: {{data.stationStatus}}</p>
						<p>No of Times Visited : {{data.counter}}</p>
					</div>
				</li>
			  </ul>
		</div>
	</div>
